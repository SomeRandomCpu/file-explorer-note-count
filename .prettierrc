{
    "singleQuote": true,
    "tabWidth": 4,
    "printWidth": 155,
    "semi": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxBracketSameLine": true,
    "overrides": [
        {
            "files": ".prettierrc",
            "options": {
                "parser": "json"
            }
        },
        {
            "files": "*.yml",
            "options": {
                "tabWidth": 4,
                "singleQuote": true
            }
        }
    ]
}
